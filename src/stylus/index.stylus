@charset "UTF-8";
//reset
html, body, div, table, td, th, tr, input, button, h1, h2, h3, h4, pre{margin: 0;padding: 0;}
body{font-size: 14px;color: #333;}
i{font-style: normal}
ul, li{list-style: none}
a{text-decoration: none;color: #333}
*{box-sizing: border-box; margin: 0; padding: 0}
@font-face{font-family:'PingFang SC'; }
html,body{font-family: PingFang SC,Helvetica Neue,Helvetica,Microsoft YaHei,SimSun,sans-serif}
body{overflow-y:scroll}
